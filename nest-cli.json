{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-electron/src",
  "monorepo": true,
  "root": "apps/nest-electron",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/nest-electron/tsconfig.app.json"
  },
  "projects": {
    "nest-electron": {
      "type": "application",
      "root": "apps/nest-electron",
      "entryFile": "main",
      "sourceRoot": "apps/nest-electron/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-electron/tsconfig.app.json"
      }
    },
    "nest-admin": {
      "type": "application",
      "root": "apps/nest-admin",
      "entryFile": "main",
      "sourceRoot": "apps/nest-admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-admin/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "nest-shop": {
      "type": "application",
      "root": "apps/nest-shop",
      "entryFile": "main",
      "sourceRoot": "apps/nest-shop/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-shop/tsconfig.app.json"
      }
    }
  }
}